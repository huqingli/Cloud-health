apply plugin: 'com.android.application'

android {
    compileSdkVersion 15
    buildToolsVersion "26.0.2"

    aaptOptions.cruncherEnabled=false
    aaptOptions.useNewCruncher=false

    defaultConfig {
        applicationId "com.moy.activity"
        minSdkVersion 11
        targetSdkVersion 18
    }

    buildTypes {
        release {
            minifyEnabled false
            proguardFiles getDefaultProguardFile('proguard-android.txt'), 'proguard-rules.txt'
        }
    }
}

repositories{
    flatDir{
        dirs 'libs' //就是你放aar的目录地址
    }
}

dependencies {
    compile 'com.android.support:recyclerview-v7:25.2.0'
    compile files('libs/achartengine-0.7.0.jar')
    compile files('libs/ThinkGearBase.jar')
    compile files('libs/ThinkGearPackX.jar')
    compile name: 'SMSSDK-3.0.0', ext: 'aar'
    compile files('libs/MobCommons-2017.0607.1736.jar')
    compile files('libs/MobTools-2017.0607.1736.jar')
}


